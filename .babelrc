{
  "presets": [
    "react",
    "es2015",
    "es2016",
    "es2017",
    "stage-0"
  ],
  "plugins": [
    "syntax-async-functions",
    "transform-regenerator",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
 ],
 "env": {
   "development": {
     "presets": [ "power-assert" ]
   }
 },
 "ignore": ["node_modules"]
}
