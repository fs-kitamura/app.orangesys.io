    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "classnames": "^2.2.5",
    "cross-env": "^2.0.1",
    "css-loader": "^0.23.1",
    "del-cli": "^0.2.0",
