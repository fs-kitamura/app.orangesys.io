{
  "name": "app.orangesys.io",
  "version": "0.1.0",
  "description": "app.orangesys.io site",
  "repository": {
    "type": "git",
    "url": "https://github.com/orangesys/app.orangesys.io"
  },
  "scripts": {
    "clean": "del-cli ./target",
    "dev": "env $(cat .env | xargs) webpack-dev-server --progress",
    "lint": "eslint -c .eslintrc.yml *.js ./src",
    "start": "npm run dev",
    "test": "env $(cat .env | xargs) NODE_PATH=. NODE_ENV=test mocha",
    "test:functions": "cd firebase-functions; yarn && npm test",
    "build": "webpack -p --progress",
    "functions:install": "cd firebase-functions; yarn",
    "functions:check": "cd firebase-functions; npm run check",
    "functions:test": "cd firebase-functions; npm test",
    "functions:build": "rm -fr functions/* && babel firebase-functions/src --out-dir functions && cp -f firebase-functions/package.json functions/package.json && (cd functions/ && npm install --production --silent)",
    "functions:deploy": "npm run functions:build && firebase deploy --only functions"
  },
  "engines": {
    "node": ">=6.4"
  },
  "devDependencies": {
